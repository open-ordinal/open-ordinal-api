{
    "$schema": "https://typedoc-plugin-markdown.org/schema.json",
    "membersWithOwnFile": [
        "Enum",
        "Variable",
        "Function",
        "Class",
        "Interface",
        "TypeAlias"
    ],
    "excludeExternals": true,
    "entryPoints": [
        "src//*/OOAPI.ts",
        "src//*/OOMD.ts"
    ],
    "entryPointStrategy": "resolve",
    "alwaysCreateEntryPointModule": true,
    "outputFileStrategy": "members",
    "hidePageHeader": true,
    "hideGroupHeadings": true,
    "textContentMappings": {
        "breadcrumbs.home": "Open Ordial API"
    },
    "readme": "src/docs/Introduction.md",
    "projectDocuments": [
        "src/docs/Introduction.md",
        "src/docs/Concepts.md",
        "src/docs/Guides.md",
        "src/docs/HowTos.md",
        "src/docs/Examples.md",
        "src/docs/Inscriptions.md",
    ],
    "out": "docs/markdown",
    "mergeReadme": true,
    "plugin": [
        "typedoc-plugin-markdown",
        "typedoc-plugin-mermaid"
    ]
}